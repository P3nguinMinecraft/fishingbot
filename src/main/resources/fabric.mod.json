{
  "schemaVersion": 1,
  "id": "fishingbot",
  "version": "${version}",
  "name": "Fishingbot",
  "description": "This is Fishingbot: The No-Fuss, Just-Works Auto-Fishing Mod for Fabric!",
  "authors": [
    "koutsie"
  ],
  "contact": {
    "homepage": "https://the-sauna.icu/Fishingbot/",
    "sources": "https://git.sr.ht/~koutsie/fishingbot",
    "issues": "https://git.sr.ht/~koutsie/fishingbot"
  },
  "license": "MIT",
  "icon": "assets/fishingbot/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "icu.thesauna.fishingbot.FishingbotClient"
    ],
    "modmenu": [
      "icu.thesauna.fishingbot.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "fishingbot.mixins.json"
  ],
  "accessWidener": "fishingbot.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.5",
